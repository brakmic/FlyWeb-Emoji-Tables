    "@types/hammerjs": "^2.0.34",
    "@types/jquery": "latest",
    "@types/lodash": "latest",
    "@types/node": "^7.0.29",
    "@types/ractive": "^0.7.27",
    "@types/requirejs": "^2.1.29",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.29",
    "@types/webpack": "^2.2.15",
    "andlog": "^1.0.0",
    "assets-webpack-plugin": "^3.5.1",
