    "bootstrap-sass": "^3.3.7",
    "circular-json": "^0.3.1",
    "codelyzer": "^3.0.1",
    "compression-webpack-plugin": "^0.4.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "domready": "^1.0.8",
    "es6-module-loader": "^0.17.11",
